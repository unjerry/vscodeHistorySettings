{
    "editor.formatOnSave": true,
    "git.autofetch": true,
    "git.autofetchPeriod": 300,
    "latex-workshop.formatting.latex": "latexindent",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
